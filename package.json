{
    "name": "vscode-test-view",
    "displayName": "vscode-test-view",
    "description": "",
    "version": "0.0.1",
    "publisher": "Etienne",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:testView.launch"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "testView.launch",
            "title": "Switch between Code file and Test file."
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}